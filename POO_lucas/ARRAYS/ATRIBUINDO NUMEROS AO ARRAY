import java.util.Scanner;
public class Main
{
	public static void main(String[] args) {
	    Scanner sc = new Scanner(System.in);
	    int numero[] = new int [8];
	    for (int i = 0; i < 8; i++)
	    {
	        System.out.println("Digite o " + (i +1) + "Â° numero.");
	            numero[i] = sc.nextInt();
	    }
		for (int i = 0; i < 8; i++)
		{
		    System.out.println("Numero " + (i + 1) + ": "  + numero[i]);
		}
	}
}
