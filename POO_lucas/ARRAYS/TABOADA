import java.util.Scanner;
public class Main
{
	public static void main(String[] args) {
	    Scanner sc = new Scanner(System.in);
	    
	    System.out.println("Digite um numero para o calculo da tabuada.");
	    int n1 = sc.nextInt();
	    
	    for (int i = 0; i < 10; i++)
	    {
	        System.out.println(n1 + " X " + i + " = " + n1 * i);
	    }
	}   
}
